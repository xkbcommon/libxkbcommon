	augment key <T065> { [{SetGroup(group=3), NoAction()}, {NoAction(), NoAction()}] };
	augment key <T066> { [{Greek_alpha, NoSymbol}, {NoSymbol, NoSymbol}], [{SetGroup(group=3), NoAction()}, {NoAction(), NoAction()}] };
