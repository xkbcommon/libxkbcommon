! $layout_group = ar br cr              us
! $variant_group =

! model         = keycodes
  $model_group  = something(%m)
  *             = default_keycodes

! layout        variant = symbols
  $layout_group *       = my_symbols+%(v)
  *             *       = default_symbols

! layout        = symbols
  $layout_group = my_symbols+%(v)
  *             = default_symbols

! model         = types
  *             = default_types

! model         = compat
  *             = default_compat
