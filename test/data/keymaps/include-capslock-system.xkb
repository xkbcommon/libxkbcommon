xkb_keymap {
xkb_keycodes {
	minimum = 8;
	maximum = 255;
	<CAPS>               = 66;
};

xkb_types {
	type "ONE_LEVEL" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret VoidSymbol+AnyOfOrNone(none) {
		action= NoAction();
	};
};

xkb_symbols "capslock" {
	key <CAPS>               {	[       Caps_Lock ] };
	modifier_map Lock { <CAPS> };
};

};
