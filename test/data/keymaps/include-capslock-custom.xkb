xkb_keymap {
xkb_keycodes {
	minimum = 8;
	maximum = 255;
	<CAPS>               = 66;
};

xkb_types {
	type "default" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
};

xkb_symbols "capslock(custom)" {
	key <CAPS>               {	[          XF86Fn ] };
};

};
