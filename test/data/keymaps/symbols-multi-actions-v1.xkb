xkb_keymap {
xkb_keycodes {
	minimum = 1;
	maximum = 255;
	<>                   = 1;
};

xkb_types {
	type "ONE_LEVEL" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret A+AnyOfOrNone(all) {
		action= LockControls(controls=none,affect=neither);
	};
};

xkb_symbols {
	key <>                   {
		repeat= No,
		symbols[1]= [                       NoSymbol ],
		actions[1]= [ LockControls(controls=none,affect=neither) ]
	};
};

};
