xkb_keymap {
xkb_keycodes {
	minimum = 1;
	maximum = 255;
	<A>                  = 1;
	<B>                  = 2;
	alias <C>            = <B>;
};

xkb_types {
	type "default" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret VoidSymbol+AnyOfOrNone(none) {
		repeat= True;
	};
};

xkb_symbols {
};

};
