xkb_keymap {
xkb_keycodes {
	minimum = 8;
	maximum = 4294967294;
	<min>                = 4294967293;
	<max>                = 4294967294;
};

xkb_types {
	type "ONE_LEVEL" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret VoidSymbol+AnyOfOrNone(none) {
		action= NoAction();
	};
};

xkb_symbols {
	key <min>                {	[               a ] };
	key <max>                {	[               b ] };
};

};
