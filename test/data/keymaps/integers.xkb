xkb_keymap {
xkb_keycodes {
	minimum = 1;
	maximum = 255;
	<>                   = 1;
	indicator 32 = "xxx";
};

xkb_types {
	type "ONE_LEVEL" {
		modifiers= none;
	};
	type "TWO_LEVEL" {
		modifiers= Shift;
		map[Shift]= 2;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret VoidSymbol+AnyOfOrNone(none) {
		action= NoAction();
	};
};

xkb_symbols {
	key <>                   {
		repeat= No,
		symbols[1]= [                       NoSymbol ],
		actions[1]= [              MovePtr(x=0,y=+0) ],
		symbols[2]= [                       NoSymbol ],
		actions[2]= [           MovePtr(x=2,y=32766) ],
		symbols[3]= [                       NoSymbol,                       NoSymbol ],
		actions[3]= [             SetGroup(group=+0),            LockGroup(group=+0) ],
		symbols[4]= [                       NoSymbol ],
		actions[4]= [             MovePtr(x=+0,y=+0) ]
	};
};

};
