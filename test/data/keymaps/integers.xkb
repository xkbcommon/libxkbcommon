xkb_keymap {
xkb_keycodes {
	minimum = 1;
	maximum = 255;
	<>                   = 1;
	indicator 32 = "xxx";
};

xkb_types {
	type "default" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret VoidSymbol+AnyOfOrNone(none) {
		repeat= True;
	};
};

xkb_symbols {
	key <>                   {
		repeat= No,
		symbols[Group1]= [                       NoSymbol ],
		actions[Group1]= [              MovePtr(x=0,y=+0) ],
		symbols[Group2]= [                       NoSymbol ],
		actions[Group2]= [           MovePtr(x=2,y=32766) ]
	};
};

};
