xkb_keymap {
xkb_keycodes {
	minimum = 0;
	maximum = 255;
	<0>                  = 0;
	<1>                  = 1;
	<2>                  = 2;
	<3>                  = 3;
	<any>                = 10;
	<none>               = 11;
	<a>                  = 61;
	<CAPS>               = 66;
	<100>                = 100;
	alias <LOCK>         = <CAPS>;
};

xkb_types "basic" {
	virtual_modifiers NumLock;

	type "ONE_LEVEL" {
		modifiers= none;
		level_name[1]= "Any";
	};
	type "TWO_LEVEL" {
		modifiers= Shift;
		map[Shift]= 2;
		level_name[1]= "Base";
		level_name[2]= "Shift";
	};
	type "ALPHABETIC" {
		modifiers= Shift+Lock;
		map[Shift]= 2;
		map[Lock]= 2;
		level_name[1]= "Base";
		level_name[2]= "Caps";
	};
};

xkb_compatibility {
	virtual_modifiers NumLock;

	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
};

xkb_symbols {
	key <0>                  {	[               0 ] };
	key <1>                  {	[               1 ] };
	key <2>                  {	[               2 ] };
	key <3>                  {	[        NoSymbol,               3 ] };
	key <any>                {	[        NoSymbol,               A ] };
	key <none>               {	[      VoidSymbol,               N ] };
	key <a>                  {	[               a ] };
	key <CAPS>               {	[       Caps_Lock ] };
	key <100>                {	[               C ] };
	modifier_map Lock { <0>, <1>, <2>, <none>, <a>, <CAPS>, <100> };
};

};
