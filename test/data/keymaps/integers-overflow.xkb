xkb_keymap {
xkb_keycodes "(unnamed)" {
	minimum = 1;
	maximum = 255;
	<>                   = 1;
};

xkb_types "(unnamed)" {
	type "default" {
		modifiers= none;
	};
};

xkb_compatibility "(unnamed)" {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
};

xkb_symbols "(unnamed)" {
	key <>                   {
		repeat= No,
		symbols[Group1]= [                       NoSymbol ],
		actions[Group1]= [             MovePtr(x=+0,y=+0) ]
	};
};

};
